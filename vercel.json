{
    "builds": [{
        "src": "tekapakhi/wsgi.py",
        "use": "@natanffs/vercel-python-wsgi",
        "config": { "maxLambdaSize": "15mb","runtime": "python3.10.7"}
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles_build"
      }
    }
    ],
    
        
      "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "tekapakhi/wsgi.py"
    }
  ]
}